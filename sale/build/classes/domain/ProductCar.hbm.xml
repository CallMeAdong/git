    <hibernate-mapping package="domain" >
	<class name="ProductCar" table="productcar" >
		<id name="pid"  >
			<generator class="assigned"></generator>
		</id>
		<property name="sid"  ></property>
		<property name="uid"  ></property>
		<property name="pname"  ></property>
		<property name="price"  ></property>
		<property name="pimg"  ></property>
		<property name="kinds"  ></property>
		<property name="info"  ></property>
		<property name="phone"  ></property>
	</class>
</hibernate-mapping>